<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', students_path  %></li>
      <li><%= link_to 'New', new_student_path %></li>
      <li class="active"><%= link_to 'Show', student_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Club_id:</b>
          <%= @student.club_id %>
        </p>

        <p>
          <b>Sname:</b>
          <%= @student.sname %>
        </p>

        <p>
          <b>Fname:</b>
          <%= @student.fname %>
        </p>

        <p>
          <b>Personal_number:</b>
          <%= @student.personal_number %>
        </p>

        <p>
          <b>Email:</b>
          <%= @student.email %>
        </p>

        <p>
          <b>Home_phone:</b>
          <%= @student.home_phone %>
        </p>

        <p>
          <b>Mobile_phone:</b>
          <%= @student.mobile_phone %>
        </p>

        <p>
          <b>Street:</b>
          <%= @student.street %>
        </p>

        <p>
          <b>Zipcode:</b>
          <%= @student.zipcode %>
        </p>

        <p>
          <b>City:</b>
          <%= @student.city %>
        </p>

        <p>
          <b>Title:</b>
          <%= @student.title %>
        </p>

        <p>
          <b>Comments:</b>
          <%= @student.comments %>
        </p>

        <p>
          <b>Password:</b>
          <%= @student.password %>
        </p>
      <%= link_to 'Edit', edit_student_path(@student) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>