<div class="block">
	<div class="secondary-navigation">
		<ul>
			<%= render :partial => 'application/links' %>
			<li class="active"><%= link_to t(:Edit) + " " + h(@club.name), edit_club_path %></li>
		</ul>
		<div class="clear"></div>
	</div>
	<div class="content">
		<h2 class="title"><%=t(:Edit_Club)%></h2>
		<div class="inner">
			<% form_for @club, :url => club_path(@club), :html => { :class => :form } do |f| -%>
			<%= render :partial => "form", :locals => {:f => f} %>
			<% end -%>
		</div>
	</div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
