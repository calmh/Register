<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', graduations_path  %></li>
      <li><%= link_to 'New', new_graduation_path %></li>
      <li class="active"><%= link_to 'Show', graduation_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Student_id:</b>
          <%= @graduation.student_id %>
        </p>

        <p>
          <b>Grade:</b>
          <%= @graduation.grade %>
        </p>

        <p>
          <b>Instructor:</b>
          <%= @graduation.instructor %>
        </p>

        <p>
          <b>Examiner:</b>
          <%= @graduation.examiner %>
        </p>

        <p>
          <b>Graduated:</b>
          <%= @graduation.graduated %>
        </p>
      <%= link_to 'Edit', edit_graduation_path(@graduation) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>