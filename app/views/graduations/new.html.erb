<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="active"><%= link_to 'New', new_student_graduation_path(@student) %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">New Graduation</h2>
    <div class="inner">
      <% form_for :graduation, :url => student_graduations_path(@student), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
