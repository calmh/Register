<div class="block">
  <div class="secondary-navigation">
    <ul>
      <%= render :partial => 'application/links' %>
      <li class="active"><%= link_to 'Edit', edit_graduation_path %></li>
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content">
    <h2 class="title">Edit Graduation</h2>
    <div class="inner">
      <% form_for @graduation, :url => graduation_path(@graduation), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>