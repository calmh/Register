<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to t(:Mailing_Lists), mailing_lists_path %></li>
      <li class="active last"><%= link_to t(:Edit_Mailing_List), edit_mailing_list_path %></li>
    </ul>
    <div class="clear"></div>
  </div>
  <div class="content">
    <h2 class="title"><%=t:Edit_Mailing_List%></h2>
    <div class="inner">
      <% form_for @mailing_list, :url => mailing_list_path(@mailing_list), :html => { :class => :form } do |f| -%>
      <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
